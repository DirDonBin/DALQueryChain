<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>DALQueryChain</Title>
    <Authors>etai-group</Authors>
    <Copyright>etai-group</Copyright>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryType>git</RepositoryType>
    <PackageTags>DALQueryChain, QueryChain, Linq2Db, DAL</PackageTags>
    <RepositoryUrl>https://github.com/DirDonBin/DALQueryChain</RepositoryUrl>
    <PackageProjectUrl>https://github.com/DirDonBin/DALQueryChain</PackageProjectUrl>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageOutputPath>..\nuget</PackageOutputPath>
    <VersionPrefix>1.0.14</VersionPrefix>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Interfaces\QueryBuilder\IUpdatableSetterQueryChain.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="Interfaces\QueryBuilder\IUpdatableSetterQueryChain.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
  </ItemGroup>

</Project>
